{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "eslint-config-airbnb"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {"no-undef": 0, "no-console": 0, "arrow-parens": 0, "no-use-before-define": 0, "consistent-return": 0},
  "ignorePatterns": ["node_modules/", "./*.js", "src/*.js"],
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true
  }
}
